#include<iostream>

using namespace std;

void myfunction();
void myname();

int main()
{
   myfunction();
   myname();
   
   cout << "\n";
   system ("PAUSE");
}

void myfunction()
{
   cout << "This is my first function in C++...\nHurray...";
}
void myname()
{
   cout << "\n\nMy Name is Vijay Prasad"; 	
}
